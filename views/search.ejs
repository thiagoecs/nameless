<div class="search__header">
  <h3>Searching for: <%=searchingBy%></h3>
</div>
<% for (post of posts) { %>
<section class="movie">
  <div class="wrapper">
    <div class="movie_header">
      <h4><a href='posts/<%=post.id%>'><%=post.restaurant%></a></h4>
      <h5><%=post.nickname%></h5>
    </div>
    <figure>
      <a href='posts/<%=post.id%>'>
        <img src="<%=post.sourceFile%>" alt=""></a>
    </figure>
    <h5>views: <%=post.views%></h5>
    <h5>comments:</h5>
    <h5>Vote: <%=post.votes%></h5>
  </div>
</section>
<% }%>
<link rel="stylesheet" href="../css/search.css" />